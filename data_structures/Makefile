
default:
	echo "Please specify which main you want to make."

main: main.c linked_list.c
	gcc main.c linked_list.c

main2: main2.c linked_list.c hash_table.c
	gcc main2.c linked_list.c hash_table.c

main3: main3.c linked_list.c read_file.c
	gcc main3.c linked_list.c read_file.c
	
run: a.out
	./a.out
